<div class="row">
	<%= link_to 'Book New Seat', new_booking_path, :class => "btn btn-primary" %>
</div>
<div class="row">
	<table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Desk</th>
        <th>From/To</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
    	<% @staffs.each do |staff| %>
	      <tr>
	        <td> <%= staff.name %> </td>
	        <td> <%= staff.bookings.last.desk.name %> </td>
	        <td> <%= "#{staff.bookings.last.from_date.strftime('%d %b')} - #{staff.bookings.last.to_date.strftime('%d %b')}" %> </td>
	        <td> <%= link_to 'View', staff_path(staff) %> </td>
	      </tr>
      <% end %>
    </tbody>
  </table>
</div>
