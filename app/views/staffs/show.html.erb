<div class="well col-lg-12">
	<div class='row'> Name: <%= @staff.name %> </div>
	<div class='row'> Email: <%= @staff.email %> </div>
	<div class='row'> Desk: <%= @staff.bookings.last.try(:desk).try(:name) %> </div>
	<div class='row'> Date: <%= "#{@staff.bookings.last.try(:from_date).try(:strftime, '%d %b')} - #{@staff.bookings.last.try(:to_date).try(:strftime, '%d %b')}" %> </div>
</div>

<div class="well col-lg-12">
	<%= link_to 'Back', current_staff.admin? ? staffs_path : root_path %>
</div>
